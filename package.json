{
  "name": "suite-territoriale-home",
  "description": "La Suite territoriale - Accueil",
  "version": "0.1.0",
  "private": true,
  "author": "ANCT",
  "license": "MIT",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "predev": "rm -rf .next && npm run only-include-used-icons && node -r @swc-node/register scripts/warm-api.ts &",
    "prebuild": "rm -rf .next && node -r @swc-node/register scripts/prebuild.ts && npm run only-include-used-icons",
    "only-include-used-icons": "node node_modules/@codegouvfr/react-dsfr/bin/only-include-used-icons.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "lint": "prettier --write '**/*.{ts,tsx,css,md,mdx,mjs,js}' --print-width 100 && next lint --no-cache && npm run lint:types",
    "lint:check": "prettier '**/*.{ts,tsx,css,md,mdx,mjs,js}' --list-different --print-width 100 && next lint --no-cache && npm run lint:types",
    "lint:types": "tsc --noEmit",
    "seo-check": "node scripts/seo-check.mjs",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:links": "start-server-and-test 'npm run dev' 3000 'linkinator 'http://localhost:3000' --recurse --skip \"^(?!http://localhost:3000)\"'",
    "test:e2e:install": "playwright install",
    "test:e2e": "playwright test",
    "test:playwright": "NEXT_PUBLIC_BASE_PATH='' NODE_ENV=production npm run build && playwright test",
    "db:drop": "node -r @swc-node/register scripts/db-drop.ts",
    "db:browse": "drizzle-kit studio --host=0.0.0.0 --port=8954",
    "db:push": "drizzle-kit push",
    "db:seed:sample": "node -r @swc-node/register scripts/db-seed.ts ./data/dumps-sample",
    "db:seed": "node -r @swc-node/register scripts/db-seed.ts ./data/dumps",
    "db:migrate": "node -r @swc-node/register scripts/db-migrate.ts"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "1.26.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.1",
    "@sentry/nextjs": "^10.3.0",
    "@socialgouv/matomo-next": "^1.9.2",
    "@swc-node/register": "^1.10.10",
    "@turf/turf": "^7.2.0",
    "@vis.gl/react-maplibre": "^8.0.4",
    "csv-stringify": "^6.6.0",
    "d3": "^7.9.0",
    "debounce": "^2.2.0",
    "drizzle-orm": "^0.44.4",
    "grist-api": "^0.1.7",
    "html-to-text": "^9.0.5",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "leaflet": "^1.9.4",
    "maplibre-gl": "^5.6.2",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "next-seo": "^6.8.0",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^7.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-map-gl": "^8.0.4",
    "rehype-parse": "^9.0.1",
    "rehype-react": "^8.0.0",
    "sharp": "^0.34.3",
    "tss-react": "^4.9.19"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@swc/core": "^1.13.3",
    "@swc/helpers": "^0.5.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/d3": "^7.4.3",
    "@types/html-to-text": "^9.0.4",
    "@types/leaflet": "^1.9.20",
    "@types/node": "24.2.0",
    "@types/pg": "^8.15.5",
    "@types/react": "19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "cheerio": "^1.1.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "jsdom": "^26.1.0",
    "linkinator": "^6.1.4",
    "next-router-mock": "^1.0.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "start-server-and-test": "2.0.13",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-musl": "4.46.2",
    "@rollup/rollup-linux-x64-gnu": "4.46.2",
    "@swc/core-linux-x64-gnu": "1.13.5",
    "@swc/core-linux-x64-musl": "1.13.5"
  }
}